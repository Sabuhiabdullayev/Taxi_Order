
@model ContactUsAllList
@{
    ViewData["Title"] = "ContactUs All List";
      Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
 <!-- Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <div class="page-breadcrumb">
                <div class="row align-items-center">
                    <div class="col-md-6 col-8 align-self-center">
                        <h3 class="page-title mb-0 p-0">Taxi</h3>
                        <div class="d-flex align-items-center">
                            <nav aria-label="breadcrumb">
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="/Admin/Order/AllList">Ana Seyfe</a></li>
                                    <li class="breadcrumb-item active" aria-current="page">Göndərilən mesaj Listəsi</li>
                                </ol>
                            </nav>
                        </div>
                    </div>
                  
                </div>
            </div>
            <!-- ============================================================== -->
            <!-- End Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Container fluid  -->
            <!-- ============================================================== -->
            <div class="container-fluid">

<h3 class="text-center">Mesaj Göndərmə Tarixi - @Model.Date</h3>

	

	<div >
        <span >Göndərən Adı</span>
	<b class="form-control">@Model.Name</b>
    </div>	
 
    <div >
        <span >Email</span>
	<b class="form-control">@Model.Email</b>
    </div>	

    <div >
        <span >Mövzu</span>
	<b class="form-control">@Model.Subject</b>
    </div>

    <div >
        <span >Mesaj</span>
	<b class="form-control" style="min-height:400px; max-height:400px; overflow:hidden; overflow:auto">@Model.Content</b>
    </div>

	

            </div>
            <!-- ============================================================== -->
            <!-- End Container fluid  -->

